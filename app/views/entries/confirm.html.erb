<p id="notice"><%= notice %></p>
<p>
  <strong>UserName:</strong>
  <%= @entry.user.name %>
</p>
<p>
  <strong>UserEmail:</strong>
  <%= @entry.user.email %>
</p>
<p>
  <strong>ReservedDate:</strong>
  <%= @entry.reserved_date %>
</p>
<p>
  <strong>UsageTime:</strong>
  <%= @entry.usage_time %>
</p>
<p>
  <strong>People:</strong>
  <%= @entry.people %>
</p>
<%= form_with(model: @entry, local: true) do |form| %>
  <%= render "entries/form",form: form %>
  <div class="actions">
    <%= form.submit '確認OK' %>
  </div>
<% end %>
<%= form_with(model: @entry, local: true, url: confirm_back_entries_path) do |form| %>
  <%= render "entries/form",form: form %>
  <div class="actions">
    <%= form.submit '確認NG' %>
  </div>
<% end %>
